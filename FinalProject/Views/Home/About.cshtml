@model IEnumerable<FinalProject.Viewmodel.OrderDateGroup>

@{
    ViewBag.Title = "Student Body Statistics";
}

<h2>會員加入日期</h2>

<table class="table">
    <tr>
        <th>
            加入日期
        </th>
        <th>
            會員數量
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @item.MemberCount
            </td>
        </tr>
    }
</table>